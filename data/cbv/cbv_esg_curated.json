{
  "metadata": {
    "title": "ESG-Focused CBV Vocabularies (Curated)",
    "description": "Curated subset of GS1 Core Business Vocabulary focusing on ESG traceability, sustainability, and regulatory compliance use cases",
    "source": "https://ref.gs1.org/cbv/",
    "standards": ["EPCIS 2.0", "CBV 2.0"],
    "curated_date": "2025-12-10",
    "esg_relevance": "HIGH",
    "use_cases": [
      "EUDR origin traceability",
      "CSRD Scope 3 supply chain mapping",
      "PPWR packaging lifecycle tracking",
      "Circular economy material flows"
    ]
  },
  "bizSteps": {
    "description": "Business process steps - populate the bizStep field in EPCIS events",
    "esg_critical": [
      {
        "code": "BizStep-commissioning",
        "uri": "https://ref.gs1.org/cbv/BizStep-commissioning",
        "label": "Commissioning",
        "description": "Process of associating an instance-level identifier with a specific object. Encompasses catching (fish), harvesting (crops), picking (produce), slaughtering (livestock).",
        "esg_use_cases": ["EUDR origin tracking", "CSRD raw material sourcing", "Farm-to-fork traceability"],
        "regulation_mapping": ["EUDR", "CSRD", "Organic Regulation"]
      },
      {
        "code": "BizStep-shipping",
        "uri": "https://ref.gs1.org/cbv/BizStep-shipping",
        "label": "Shipping",
        "description": "Process of staging outbound, loading, and departing. Used when granular process step information is unknown.",
        "esg_use_cases": ["CSRD Scope 3 transport emissions", "Supply chain mapping"],
        "regulation_mapping": ["CSRD", "CBAM"]
      },
      {
        "code": "BizStep-receiving",
        "uri": "https://ref.gs1.org/cbv/BizStep-receiving",
        "label": "Receiving",
        "description": "Process of accepting goods at a destination location.",
        "esg_use_cases": ["Custody chain verification", "EUDR due diligence checkpoints"],
        "regulation_mapping": ["EUDR", "CSRD"]
      },
      {
        "code": "BizStep-transforming",
        "uri": "https://ref.gs1.org/cbv/BizStep-transforming",
        "label": "Transforming",
        "description": "Process where input objects are consumed to produce output objects (manufacturing, processing).",
        "esg_use_cases": ["EUDR product transformation tracking", "CSRD manufacturing emissions", "PPWR material composition"],
        "regulation_mapping": ["EUDR", "CSRD", "PPWR"]
      },
      {
        "code": "BizStep-destroying",
        "uri": "https://ref.gs1.org/cbv/BizStep-destroying",
        "label": "Destroying",
        "description": "Process of terminating an object (disposal, incineration).",
        "esg_use_cases": ["PPWR end-of-life tracking", "Circular economy waste flows"],
        "regulation_mapping": ["PPWR", "Waste Framework Directive"]
      },
      {
        "code": "BizStep-recycling",
        "uri": "https://ref.gs1.org/cbv/BizStep-recycling",
        "label": "Recycling",
        "description": "Process of converting waste materials into new materials and objects.",
        "esg_use_cases": ["PPWR recycled content verification", "Circular economy material loops"],
        "regulation_mapping": ["PPWR", "Circular Economy Action Plan"]
      },
      {
        "code": "BizStep-repairing",
        "uri": "https://ref.gs1.org/cbv/BizStep-repairing",
        "label": "Repairing",
        "description": "Process of restoring an object to a functional state.",
        "esg_use_cases": ["PPWR product lifetime extension", "Right to repair compliance"],
        "regulation_mapping": ["PPWR", "Ecodesign Directive"]
      },
      {
        "code": "BizStep-collecting",
        "uri": "https://ref.gs1.org/cbv/BizStep-collecting",
        "label": "Collecting",
        "description": "Process of picking up objects for future disposal, recycling, or reuse.",
        "esg_use_cases": ["PPWR collection schemes", "EPR compliance tracking"],
        "regulation_mapping": ["PPWR", "EPR Directive"]
      }
    ]
  },
