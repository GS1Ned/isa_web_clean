  "esg_mapping_guide": {
    "eudr_traceability_chain": [
      "BizStep-commissioning (origin)",
      "SDT-location (geographic coordinates)",
      "BTT-cert (FSC/PEFC certification)",
      "BizStep-transforming (processing steps)",
      "SDT-owning_party (custody chain)",
      "BizStep-shipping + BizStep-receiving (transport)"
    ],
    "csrd_scope3_emissions": [
      "BizStep-shipping (transport)",
      "Disp-in_transit (goods in transit)",
      "MeasurementType-Speed (vehicle speed)",
      "MeasurementType-Mileage (distance)",
      "SDT-location (facility locations)",
      "BizStep-transforming (manufacturing emissions)"
    ],
    "ppwr_circular_economy": [
      "BizStep-commissioning (production)",
      "Disp-active (in use)",
      "BizStep-collecting (collection)",
      "BizStep-recycling (recycling process)",
      "Disp-recyclable (recyclable status)",
      "BizStep-destroying (disposal)",
      "Disp-returned (take-back)"
